%h1 New Schools Volunteer
-if params[:volunteer_id]
	%h4= "#{@volunteer.vol_id}: #{@volunteer.name}"
- elsif params[:school_id]
	%h4= "#{@school.name}"
- else
- if params[:volunteer_id]
	= link_to "Cancel", volunteer_path(@volunteer), class: "btn btn-danger btn-sm"
- elsif params[:school_id]
	= link_to "Cancel", school_path(@school), class: "btn btn-danger btn-sm"
%br
%br
= render 'form'
