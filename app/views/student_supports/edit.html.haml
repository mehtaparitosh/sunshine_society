%h1 Edit Students After School Support
-if params[:student_id] 
	%h4= "#{@student.st_id}: #{@student.name}"
- elsif params[:support_id]
	%h4= "#{@support.name}"
- else
= render 'form'
- if params[:student_id] 
	= link_to "Cancel", student_path(@student)
- elsif params[:support_id]
	= link_to "Cancel", support_path(@support)