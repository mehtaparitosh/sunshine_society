%h1 Create Student's After School Support
-if params[:student_id]
	%h4= "#{@student.st_id}: #{@student.name}"
- elsif params[:support_id]
	%h4= "#{@support.name}"
- else
- if params[:student_id]
	= link_to "Cancel", student_path(@student), class: "btn btn-danger btn-sm"
- elsif params[:support_id]
	= link_to "Cancel", support_path(@support), class: "btn btn-danger btn-sm"
%br
%br
= render 'form'
