%h1 Edit Volunteer for Student
-if params[:student_id] 
	%h4= "#{@student.st_id}: #{@student.name}"
- elsif params[:volunteer_id]
	%h4= "#{@volunteer.vol_id}: #{@volunteer.name}"
- else
= render 'form'
- if params[:student_id] 
	= link_to "Cancel", student_path(@student)
- elsif params[:volunteer_id]
	= link_to "Cancel", volunteer_path(@volunteer)