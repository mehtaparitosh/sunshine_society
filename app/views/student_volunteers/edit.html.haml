%h1 Edit Volunteer for Student
-if params[:student_id]
	%h4= "#{@student.st_id}: #{@student.name}"
- elsif params[:volunteer_id]
	%h4= "#{@volunteer.vol_id}: #{@volunteer.name}"
- else
- if params[:student_id]
	= link_to "Cancel", student_path(@student), class: "btn btn-danger btn-sm"
- elsif params[:volunteer_id]
	= link_to "Cancel", volunteer_path(@volunteer), class: "btn btn-danger btn-sm"
%br
%br

= render 'form'
