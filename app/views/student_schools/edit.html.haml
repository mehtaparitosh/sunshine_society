%h1 Edit Students School
-if params[:student_id] 
	%h4= "#{@student.st_id}: #{@student.name}"
- elsif params[:school_id]
	%h4= "#{@school.name}"
- else
= render 'form'
- if params[:student_id] 
	= link_to "Cancel", student_path(@student)
- elsif params[:school_id]
	= link_to "Cancel", school_path(@school)